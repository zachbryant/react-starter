{
  "name": "react-typescript-tailwind-parcel",
  "private": "true",
  "version": "1.0.0",
  "homepage": "https://github.com/zachbryant/react-starter",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all -s clean -p watch:css dev",
    "dev": "parcel src/index.html",
    "prod": "npm-run-all -s clean -p build:css build",
    "build": "parcel build src/index.html",
    "test": "echo TEST SCRIPT TODO",
    "clean": "rm -rf dist/ .cache/",
    "purge": "npm run clean && rm -rf node_modules/",
    "build:css": "postcss src/**/*.sass src/**/*.scss src/**/*.css --base src/ --dir dist/",
    "watch:css": "postcss src/**/*.sass src/**/*.scss src/**/*.css --base src/ --dir dist/ -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/recoil": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^10.2.4",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-typescript-paths": "file:./parcel-plugin-typescript-paths/dist/",
    "postcss": "^8.2.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-scss": "^3.0.4",
    "postcss-url": "^10.1.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.7",
    "tsconfig.js": "^3.0.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "recoil": "^0.1.2",
    "rxjs": "^6.6.3",
    "tailwindcss": "^2.0.3"
  }
}
