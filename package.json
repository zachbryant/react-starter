{
  "name": "frontend-starter",
  "version": "0.0.0",
  "repository": "git@github.com:zachbryant/react-starter.git",
  "homepage": "https://github.com/zachbryant/react-starter",
  "author": "Zach Bryant <zdb282@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "run-p watch:css dev",
    "start:clean": "npm-run-all -s clean start",
    "dev": "run-s dev:webpack",
    "dev:webpack": "nodemon -w webpack.config.cjs -w tsconfig.js --exec 'webpack serve --progress --profile'",
    "prod": "run-s clean build:tsconfig build:css build:webpack",
    "prod:webpack": "NODE_ENV=production run-s dev:webpack",
    "build": "run-s clean build:tsconfig build:css build:webpack",
    "build:webpack": "NODE_ENV=production webpack --progress --profile",
    "build:css": "NODE_ENV=production postcss src/**/*.sass src/**/*.scss src/**/*.css --base src/ --dir dist/",
    "build:tsconfig": "node tsconfig.cjs",
    "watch:css": "npm run build:css -- -w",
    "storybook": "start-storybook --modern -p 6006",
    "prebuild:storybook": "test:generate-output",
    "build:storybook": "build-storybook --modern",
    "lint": "eslint src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --passWithNoTests",
    "test:generate-output": "npm run test -- --json --outputFile=.jest-test-results.json",
    "clean": "rm -rf dist/ .cache/",
    "purge": "npm run clean && rm -rf node_modules/",
    "create:component": "npx generate-react-cli component",
    "create:layout": "npx generate-react-cli component --type=layout",
    "create:page": "npx generate-react-cli component --type=page",
    "postinstall": "rm -rf ./node_modules/.cache/webpack"
  },
  "resolutions": {
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0"
  },
