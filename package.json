{
  "name": "frontend-starter",
  "version": "1.0.0",
  "repository": "git@github.com:zachbryant/react-starter.git",
  "homepage": "https://github.com/zachbryant/react-starter",
  "author": "Zach Bryant <zdb282@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "run-p watch:css dev",
    "start:clean": "npm-run-all -s clean start",
    "dev": "nodemon -w tsconfig.js --exec \"run-s build:tsconfig dev-parcel\"",
    "dev-parcel": "parcel src/index.html",
    "dev-webpack": "nodemon -w webpack.config.js --exec 'webpack serve --progress --profile'",
    "prod": "run-s clean build:tsconfig watch:css build-parcel",
    "prod-parcel": "",
    "prod-webpack": "NODE_ENV=production nodemon -w webpack.config.js --exec 'webpack serve --progress --profile --env production'",
    "build": "run-s clean build:tsconfig watch:css build-parcel",
    "build-parcel": "parcel build src/index.html",
    "build-webpack": "NODE_ENV=production webpack --progress --profile --env production",
    "build:css": "postcss src/**/*.sass src/**/*.scss src/**/*.css --base src/ --dir dist/",
    "build:tsconfig": "node tsconfig.js",
    "watch:css": "postcss src/**/*.sass src/**/*.scss src/**/*.css --base src/ --dir dist/ -w",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "test": "echo TEST SCRIPT TODO",
    "clean": "rm -rf dist/ .cache/ .parcel-cache/",
    "purge": "npm run clean && rm -rf node_modules/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-spring": "^9.1.2",
    "recoil": "^0.2.0",
    "rfs": "^9.0.3",
    "rxjs": "^6.6.7",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@parcel/reporter-bundle-analyzer": "^2.0.0-beta.2",
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "@parcel/transformer-typescript-tsc": "^2.0.0-beta.2",
    "@parcel/validator-typescript": "^2.0.0-beta.2",
    "@storybook/addon-actions": "^6.2.9",
