{
    "name": "react-typescript-tailwind-webpack",
    "private": "true",
    "version": "2.0.0",
    "homepage": "https://github.com/zachbryant/react-starter",
    "license": "MIT",
    "scripts": {
        "start": "concurrently \"npm run watch:css\" \"npm run dev\"",
        "dev": "webpack serve --progress --profile",
        "prod": "NODE_ENV=production concurrently \"npm run watch:css\" \"webpack  --progress --profile --env production\"",
        "build": "NODE_ENV=production npm run build:css && webpack --progress --profile --env production",
        "test": "echo TEST SCRIPT TODO",
        "clean": "rm -rf node_modules/ dist/",
        "build:css": "postcss src/**/*.sass src/**/*.scss src/**/*.css --base src/ --dir dist/",
        "watch:css": "postcss src/**/*.sass src/**/*.scss src/**/*.css --base src/ --dir dist/ -w"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 edge version"
        ]
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
