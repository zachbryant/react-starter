    "license": "MIT",
    "scripts": {
        "start": "concurrently \"npm run watch:css\" \"webpack-dev-server --env.NODE_ENV=development --open\"",
        "prod": "npm run watch:css && webpack --env.NODE_ENV=production",
        "build": "npm run build:css webpack --env.NODE_ENV=production",
        "test": "echo TEST SCRIPT TODO",
        "clean": "rm -rf node_modules",
        "build:css": "postcss src/assets/tailwind.scss -o dist/index.scss",
        "watch:css": "postcss src/assets/tailwind.scss -wo dist/index.scss"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
