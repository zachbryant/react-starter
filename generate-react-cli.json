{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "index": "src/config/templates/index.ts",
        "component": "src/config/templates/TemplateName.tsx",
        "lazy": "src/config/templates/TemplateName.lazy.tsx",
        "test": "src/config/templates/TemplateName.test.tsx",
        "story": "src/config/templates/TemplateName.stories.tsx",
        "style": "src/config/templates/TemplateName.module.scss"
      },
      "path": "src/ui/fragments/",
      "withIndex": true,
      "withLazy": true,
      "withStory": true,
      "withStyle": true,
      "withTest": true
    },
    "page": {
      "customTemplates": {
        "index": "src/config/templates/index.ts",
        "component": "src/config/templates/TemplateName.tsx",
        "lazy": "src/config/templates/TemplateName.lazy.tsx",
        "test": "src/config/templates/TemplateName.test.tsx",
        "story": "src/config/templates/TemplateName.stories.tsx",
        "style": "src/config/templates/TemplateName.module.scss"
      },
      "path": "src/ui/pages/",
      "withIndex": true,
      "withLazy": true,
      "withStory": true,
      "withStyle": true,
      "withTest": true
    },
    "layout": {
      "customTemplates": {
        "index": "src/config/templates/index.ts",
        "component": "src/config/templates/TemplateName.tsx",
        "lazy": "src/config/templates/TemplateName.lazy.tsx",
        "test": "src/config/templates/TemplateName.test.tsx",
        "story": "src/config/templates/TemplateName.stories.tsx",
        "style": "src/config/templates/TemplateName.module.scss"
      },
      "path": "src/ui/layouts/",
      "withIndex": true,
      "withLazy": true,
      "withStory": true,
      "withStyle": true,
      "withTest": true
    }
  }
}
